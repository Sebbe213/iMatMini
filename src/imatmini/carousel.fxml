<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>

<fx:root fx:id="manPane" prefHeight="214.0" prefWidth="720.0" style="-fx-background-color: #FFFFFF;" type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <FlowPane fx:id="flowPane" layoutX="-4.0" prefHeight="214.0" prefWidth="733.0">
         <children>
            <HBox fx:id="productBox" prefHeight="214.0" prefWidth="723.0" spacing="40.0">
               <FlowPane.margin>
                  <Insets top="30.0" />
               </FlowPane.margin></HBox>
         </children>
      </FlowPane>
      <Button fx:id="rightButton" layoutX="677.0" layoutY="80.0" mnemonicParsing="false" onAction="#moveRight" prefHeight="60.0" prefWidth="52.0" style="-fx-background-color: #FFFFFF;" styleClass="button-style-carousel" stylesheets="@iMatMini.css">
         <graphic>
            <ImageView fitHeight="30.0" fitWidth="42.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@pics/right-arrow.png" />
               </image>
               <cursor>
                  <Cursor fx:constant="DEFAULT" />
               </cursor>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="leftButton" layoutY="80.0" mnemonicParsing="false" onAction="#moveLeft" prefHeight="60.0" prefWidth="52.0" style="-fx-background-color: #FFFFFF;" styleClass="button-style-carousel" stylesheets="@iMatMini.css">
         <graphic>
            <ImageView fitHeight="30.0" fitWidth="36.0" pickOnBounds="true" preserveRatio="true" rotate="180.0">
               <image>
                  <Image url="@pics/right-arrow.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</fx:root>
