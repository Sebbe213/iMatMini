<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>


<AnchorPane fx:id="carouselPane" prefHeight="214.0" prefWidth="720.0" style="-fx-background-color: #FFFFFF;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="imatmini.Carousel">
   <children>
      <HBox fx:id="productBox" layoutX="66.0" prefHeight="214.0" prefWidth="587.0" />
      <Button fx:id="leftButton" layoutY="90.0" mnemonicParsing="false" onAction="#moveLeft" prefHeight="38.0" prefWidth="65.0" style="-fx-background-color: #FFFFFF;">
         <graphic>
            <ImageView fitHeight="30.0" fitWidth="36.0" pickOnBounds="true" preserveRatio="true" rotate="180.0">
               <image>
                  <Image url="@pics/right-arrow.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="rightButton" layoutX="653.0" layoutY="91.0" mnemonicParsing="false" onAction="#moveRight" prefHeight="38.0" prefWidth="70.0" style="-fx-background-color: #FFFFFF;">
         <graphic>
            <ImageView fitHeight="30.0" fitWidth="42.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@pics/right-arrow.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</AnchorPane>
